      <prop type="x-BRK:MultiplePicklist">STANDARD-TXT-NBO</prop>
      <tuv xml:lang="en-GB">
        <seg>Entry into force and date of application</seg>
      </tuv>
      <tuv xml:lang="nb-NO">
